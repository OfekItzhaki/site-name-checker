{
  "name": "domain-availability-checker",
  "version": "1.0.0",
  "description": "A client-side web application that provides real-time domain availability checking across multiple TLDs",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "watch": "tsc --watch",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:property": "jest --testPathPattern=property",
    "test:coverage": "jest --coverage",
    "lint": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "start:api": "node dist/api/index.js",
    "start:frontend": "node server.js",
    "start": "npm run build && npm run start:api"
  },
  "keywords": [
    "domain",
    "availability",
    "checker",
    "dns",
    "whois",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^30.2.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "fast-check": "^3.15.0",
    "whois": "^2.13.5"
  }
}
